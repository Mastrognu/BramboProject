import 'SocialResource.ecore'

package smr

context SocialMediaResource

inv NonNull_id('The \'id\' property of "' + self.toString() + '" is null'):
	id <> null and sourcesn <> null
	
context Person

inv AuthorExists(''): Boolean = 
	self -> select(a|a.oclIsTypeOf(Author)) -> size() = 1
	
endpackage
